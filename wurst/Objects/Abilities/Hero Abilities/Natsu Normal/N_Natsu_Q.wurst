package N_Natsu_Q
import ClosureTimers
init
    edoMinatoG()
function edoMinatoG()
    CreateTrigger()
    ..registerAnyUnitEvent(EVENT_PLAYER_UNIT_SPELL_EFFECT)
    ..addAction() ->
        if GetSpellAbilityId() == 'A00I'
            let caster = GetTriggerUnit()
            caster.addAbility('A00S')
            caster.hideAbility('A00S', true)
            doAfter(0.3) -> 
                caster.hideAbility('A00S', false)