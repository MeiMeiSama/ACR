function initTrig_TeachW()
    gg_trg_TeachW = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_TeachW, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_TeachW, Condition(function trig_TeachWConditions))
    TriggerAddAction(gg_trg_TeachW, function trig_TeachWActions)