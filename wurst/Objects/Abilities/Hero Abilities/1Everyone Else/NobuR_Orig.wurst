    TriggerRegisterAnyUnitEventBJ(gg_trg_NobuR, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_NobuR, Condition(function trig_NobuRConditions))
    TriggerAddAction(gg_trg_NobuR, function trig_NobuRActions)