function initTrig_XtyF()
    gg_trg_XtyF = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_XtyF, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_XtyF, Condition(function trig_XtyFConditions))
    TriggerAddAction(gg_trg_XtyF, function trig_XtyFActions)