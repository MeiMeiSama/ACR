function initTrig_YamiQ()
    gg_trg_YamiQ = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_YamiQ, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_YamiQ, Condition(function trig_YamiQConditions))
    TriggerAddAction(gg_trg_YamiQ, function trig_YamiQActions)