function trig_YamiQFunc004Func028A()
    DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q2)
    DamageEvent.setNextDamageFromCode()
    UnitDamageTargetBJ(yamiCaster, GetEnumUnit(), 100.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
    let victim = GetEnumUnit()
    acrSlow(yamiCaster, victim, 40, 2)
function trig_YamiQFunc004Func032T()
