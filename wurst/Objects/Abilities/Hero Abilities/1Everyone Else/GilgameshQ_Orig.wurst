function trig_GilgameshQConditions() returns boolean
    return GetSpellAbilityId() == 'A0NC'
function trig_GilgameshQFunc015Func001Func030003003() returns boolean
    return IsUnitType(GetFilterUnit(), UNIT_TYPE_STRUCTURE) == false and IsUnitEnemy(GetFilterUnit(), LoadPlayerHandle(YDLOC, GetHandleId(GetExpiredTimer()), 603)) == true and GetUnitAbilityLevel(GetFilterUnit(), 'Avul') != 1 and IsUnitDeadBJ(GetFilterUnit()) == false
function trig_GilgameshQFunc015Func001Func031A()
    if GetUnitAbilityLevel(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), 'A0NC') == 1
        DamageEvent.setNextDamageId(DAMAGE_ABILITY_GILGAMESH_Q)
        DamageEvent.setNextDamageFromCode()
        UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), 25.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
    else if GetUnitAbilityLevel(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), 'A0NC') == 2
        DamageEvent.setNextDamageId(DAMAGE_ABILITY_GILGAMESH_Q)
        DamageEvent.setNextDamageFromCode()
        UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), 45.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
    else if GetUnitAbilityLevel(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), 'A0NC') == 3
        DamageEvent.setNextDamageId(DAMAGE_ABILITY_GILGAMESH_Q)
        DamageEvent.setNextDamageFromCode()
        UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), 65.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
    else if GetUnitAbilityLevel(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), 'A0NC') == 4
        DamageEvent.setNextDamageId(DAMAGE_ABILITY_GILGAMESH_Q)
        DamageEvent.setNextDamageFromCode()
        UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), 85.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
    else if GetUnitAbilityLevel(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), 'A0NC') == 5
        DamageEvent.setNextDamageId(DAMAGE_ABILITY_GILGAMESH_Q)
        DamageEvent.setNextDamageFromCode()
