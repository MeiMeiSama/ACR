function initTrig_GilgameshQ_Orig()
    gg_trg_GilgameshQ = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_GilgameshQ, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_GilgameshQ, Condition(function trig_GilgameshQConditions))
    TriggerAddAction(gg_trg_GilgameshQ, function trig_GilgameshQActions)