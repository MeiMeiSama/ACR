function initTrig_SaberW_Orig()
    gg_trg_SaberW = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_SaberW, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_SaberW, Condition(function trig_SaberWConditions))
    TriggerAddAction(gg_trg_SaberW, function trig_SaberWActions)