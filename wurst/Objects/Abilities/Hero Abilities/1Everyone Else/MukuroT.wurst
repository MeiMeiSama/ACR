function initTrig_MukuroT()
    gg_trg_MukuroT = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_MukuroT, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_MukuroT, Condition(function trig_MukuroTConditions))
    TriggerAddAction(gg_trg_MukuroT, function trig_MukuroTActions)