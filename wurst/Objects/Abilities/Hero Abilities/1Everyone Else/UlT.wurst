function initTrig_UlT()
    gg_trg_UlT = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_UlT, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_UlT, Condition(function trig_UlTConditions))
    TriggerAddAction(gg_trg_UlT, function trig_UlTActions)