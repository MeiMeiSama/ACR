function initTrig_HSaberT_Orig()
    gg_trg_HSaberT = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_HSaberT, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_HSaberT, Condition(function trig_HSaberTConditions))
    TriggerAddAction(gg_trg_HSaberT, function trig_HSaberTActions)