function initTrig_XtyR()
    gg_trg_XtyR = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_XtyR, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_XtyR, Condition(function trig_XtyRConditions))
    TriggerAddAction(gg_trg_XtyR, function trig_XtyRActions)