function initTrig_SaberShikiQ_Orig()
    gg_trg_SaberShikiQ = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_SaberShikiQ, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_SaberShikiQ, Condition(function trig_SaberShikiQConditions))
    TriggerAddAction(gg_trg_SaberShikiQ, function trig_SaberShikiQActions)