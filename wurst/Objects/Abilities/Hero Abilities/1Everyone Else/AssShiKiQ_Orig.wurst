function trig_AssShiKiQConditions() returns boolean
    return GetSpellAbilityId() == 'A0RF'
function trig_AssShiKiQFunc021Func007003003() returns boolean
    return IsUnitType(GetFilterUnit(), UNIT_TYPE_STRUCTURE) == false and IsUnitAliveBJ(GetFilterUnit()) == true and IsUnitEnemy(GetFilterUnit(), LoadPlayerHandle(YDLOC, GetHandleId(GetExpiredTimer()), 603)) == true and IsUnitInGroup(GetFilterUnit(), LoadGroupHandle(YDLOC, GetHandleId(GetExpiredTimer()), 744928215)) == false and GetUnitAbilityLevel(GetFilterUnit(), 'Avul') != 1
function trig_AssShiKiQFunc021Func008A()
    if UnitHasBuffBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), 'B04N') == true and GetUnitTypeId(GetEnumUnit()) != 'O005' and GetUnitTypeId(GetEnumUnit()) != 'O007' and GetUnitTypeId(GetEnumUnit()) != 'e0PC' and GetUnitTypeId(GetEnumUnit()) != 'e0PB' and GetUnitTypeId(GetEnumUnit()) != 'e0PD'
        if GetPlayerName(LoadPlayerHandle(YDLOC, GetHandleId(GetExpiredTimer()), 603)) != "every night"
            if IsUnitType(GetEnumUnit(), UNIT_TYPE_HERO) == true
                SaveInteger(YDLOC, GetHandleId(GetExpiredTimer()), 1153, GetRandomInt(1, 10))
                if LoadInteger(YDLOC, GetHandleId(GetExpiredTimer()), 1153) <= 6
                    DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q)
                    DamageEvent.setNextDamageFromCode()
                    UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), LoadReal(YDLOC, GetHandleId(GetExpiredTimer()), 248) + 100000.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
                else
                    DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q)
                    DamageEvent.setNextDamageFromCode()
                    UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), 500.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
            else
                DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q)
                DamageEvent.setNextDamageFromCode()
                UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), 500.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
        else if IsUnitType(GetEnumUnit(), UNIT_TYPE_HERO) == true
            SaveInteger(YDLOC, GetHandleId(GetExpiredTimer()), 1153, GetRandomInt(1, 10))
            if LoadInteger(YDLOC, GetHandleId(GetExpiredTimer()), 1153) == 2
                DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q)
                DamageEvent.setNextDamageFromCode() 
                UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), LoadReal(YDLOC, GetHandleId(GetExpiredTimer()), 248) + 100000.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
            else
                DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q)
                DamageEvent.setNextDamageFromCode()
                UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), LoadReal(YDLOC, GetHandleId(GetExpiredTimer()), 248), ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
        else
            SaveInteger(YDLOC, GetHandleId(GetExpiredTimer()), 255, GetRandomInt(1, 10))
            if LoadInteger(YDLOC, GetHandleId(GetExpiredTimer()), 255) <= 5
                DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q)
                DamageEvent.setNextDamageFromCode()
                UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), LoadReal(YDLOC, GetHandleId(GetExpiredTimer()), 248), ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
            else
                DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q)
                DamageEvent.setNextDamageFromCode()
                UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), LoadReal(YDLOC, GetHandleId(GetExpiredTimer()), 248) + 100000.00, ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
    else
        DamageEvent.setNextDamageId(DAMAGE_ABILITY_Q)
        DamageEvent.setNextDamageFromCode()
        UnitDamageTargetBJ(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 594), GetEnumUnit(), LoadReal(YDLOC, GetHandleId(GetExpiredTimer()), 248), ATTACK_TYPE_MAGIC, DAMAGE_TYPE_MAGIC)
    SaveLocationHandle(YDLOC, GetHandleId(GetExpiredTimer()), 450813563, GetUnitLoc(GetEnumUnit()))
    SaveUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 694, CreateUnitAtLoc(LoadPlayerHandle(YDLOC, GetHandleId(GetExpiredTimer()), 603), 'e0HU', LoadLocationHandle(YDLOC, GetHandleId(GetExpiredTimer()), 450813563), GetRandomDirectionDeg()))
