function initTrig_ULW()
    gg_trg_ULW = CreateTrigger()
    TriggerRegisterAnyUnitEventBJ(gg_trg_ULW, EVENT_PLAYER_UNIT_SPELL_EFFECT)
    TriggerAddCondition(gg_trg_ULW, Condition(function trig_ULWConditions))
    TriggerAddAction(gg_trg_ULW, function trig_ULWActions)