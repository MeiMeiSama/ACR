function trig_RinWFunc004Func025T()
    //group _ydl_group
    //unit _ydl_unit
    timer _ydl_timer
    PlaySoundBJ(gg_snd_Yayaeffectsound)
    SaveLocationHandle(YDLOC, GetHandleId(GetExpiredTimer()), 48, GetUnitLoc(LoadUnitHandle(YDLOC, GetHandleId(GetExpiredTimer()), 449)))
